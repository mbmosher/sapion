<h1>Matches</h1>

<%= link_to "Edit Filters", edit_search_path(@search), class: 'btn' %>
<br /><br />

<% @profiles.each do |prof| %>
    <p>
	<% if prof.photos.first %>
    <%= image_tag(prof.photos.first.avatar.match.url) %>
    <% end %>
	<%= link_to prof.first_name,  profile_path(prof) %>
	<%= prof.tagline %>
	</p>
	<% end %>
	
	